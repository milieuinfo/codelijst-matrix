#S15 	skos:notation is an instance of owl:DatatypeProperty.
        {
        ?s skos:notation ?o .
} => {
        skos:notation rdf:type owl:DatatypeProperty .
        } .
#S16 	skos:note, skos:changeNote, skos:definition, skos:editorialNote, skos:example, skos:historyNote and skos:scopeNote are each instances of owl:AnnotationProperty.
#S17 	skos:changeNote, skos:definition, skos:editorialNote, skos:example, skos:historyNote and skos:scopeNote are each sub-properties of skos:note.
        {
        ?s skos:note ?o .
} => {
        skos:note rdf:type owl:AnnotationProperty .
        } .

        {
        ?s skos:changeNote ?o .
} => {
        skos:changeNote rdf:type owl:AnnotationProperty .
?s skos:note ?o .
        } .

        {
        ?s skos:definition ?o .
} => {
        skos:definition rdf:type owl:AnnotationProperty .
?s skos:note ?o .
        } .

        {
        ?s skos:editorialNote ?o .
} => {
        skos:editorialNote rdf:type owl:AnnotationProperty .
?s skos:note ?o .
        } .

        {
        ?s skos:example ?o .
} => {
        skos:example rdf:type owl:AnnotationProperty .
?s skos:note ?o .
        } .

        {
        ?s skos:historyNote ?o .
} => {
        skos:historyNote rdf:type owl:AnnotationProperty .
?s skos:note ?o .
        } .

        {
        ?s skos:scopeNote ?o .
} => {
        skos:scopeNote rdf:type owl:AnnotationProperty .
?s skos:note ?o .
        } .

#S18 	skos:semanticRelation, skos:broader, skos:narrower, skos:related, skos:broaderTransitive and skos:narrowerTransitive are each instances of owl:ObjectProperty.
        {
        ?s skos:semanticRelation ?o .
} => {
        skos:semanticRelation rdf:type owl:ObjectProperty .
        } .

        {
        ?s skos:broader ?o .
} => {
        skos:broader rdf:type owl:ObjectProperty .
        } .

        {
        ?s skos:narrower ?o .
} => {
        skos:narrower rdf:type owl:ObjectProperty .
        } .

        {
        ?s skos:related ?o .
} => {
        skos:related rdf:type owl:ObjectProperty .
        } .

        {
        ?s skos:broaderTransitive ?o .
} => {
        skos:broaderTransitive rdf:type owl:ObjectProperty .
        } .

        {
        ?s skos:narrowerTransitive ?o .
} => {
        skos:narrowerTransitive rdf:type owl:ObjectProperty .
        } .

#S19 	The rdfs:domain of skos:semanticRelation is the class skos:Concept.
        {
        ?s skos:semanticRelation ?o .
} => {
        ?s rdf:type skos:Concept .
        } .
#S20 	The rdfs:range of skos:semanticRelation is the class skos:Concept.
        {
        ?s skos:semanticRelation ?o .
} => {
        ?o rdf:type skos:Concept .
        } .
#S21 skos:broaderTransitive, skos:narrowerTransitive and skos:related are each sub-properties of skos:semanticRelation.
                {
                ?s skos:broaderTransitive ?o .
} => {
        skos:broaderTransitive rdfs:subPropertyOf skos:semanticRelation .
?s skos:semanticRelation ?o .
        } .
        {
        ?s skos:narrowerTransitive ?o .
} => {
        skos:narrowerTransitive rdfs:subPropertyOf skos:semanticRelation .
?s skos:semanticRelation ?o .
        } .
        {
        ?s skos:related ?o .
} => {
        skos:related rdfs:subPropertyOf skos:semanticRelation .
?s skos:semanticRelation ?o .
        } .

#S22 	skos:broader is a sub-property of skos:broaderTransitive, and skos:narrower is a sub-property of skos:narrowerTransitive.
        {
        ?s skos:broader ?o .
} => {
        skos:broader rdfs:subPropertyOf skos:broaderTransitive .
?s skos:broaderTransitive ?o .
        } .
        {
        ?s skos:narrower ?o .
} => {
        skos:narrower rdfs:subPropertyOf skos:narrowerTransitive .
?s skos:narrowerTransitive ?o .
        } .
#S23 	skos:related is an instance of owl:SymmetricProperty.
        {
        ?s skos:related ?o .
} => {
        ?o skos:related ?s .
        } .
#skos:broaderTransitive and skos:narrowerTransitive are each instances of owl:TransitiveProperty.
        {
        ?s skos:broaderTransitive ?o .
} => {
        skos:broaderTransitive rdf:type  owl:TransitiveProperty .
        } .
        {
        ?s skos:narrowerTransitive ?o .
} => {
        skos:narrowerTransitive rdf:type  owl:TransitiveProperty .
        } .
#S25 	skos:narrower is owl:inverseOf the property skos:broader.
        {
        ?s skos:broader ?o .
} => {
        skos:broader owl:inverseOf skos:narrower .
        ?o skos:narrower ?s .
} .
        {
        ?s skos:narrower ?o .
} => {
        skos:narrower owl:inverseOf skos:broader .
        ?o skos:broader ?s .
        } .
#S26 	skos:narrowerTransitive is owl:inverseOf the property skos:broaderTransitive.
        {
        ?s skos:narrowerTransitive ?o .
} => {
        skos:narrowerTransitive owl:inverseOf skos:broaderTransitive .
?o skos:broaderTransitive ?s .
} .
        {
        ?s skos:broaderTransitive ?o .
} => {
        skos:broaderTransitive owl:inverseOf skos:narrowerTransitive .
?o skos:narrowerTransitive ?s .
        } .
#S27 	skos:related is disjoint with the property skos:broaderTransitive.
